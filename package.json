{
	"name": "obsidian-project-snapshot",
	"version": "1.0.0",
	"description": "Track GitHub projects directly in Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"github",
		"project-management"
	],
	"author": "Your Name",
	"license": "MIT",
	"dependencies": {
		"@octokit/rest": "^20.0.0",
		"date-fns": "^3.0.0"
	},
	"devDependencies": {
		"@types/node": "^16.0.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"builtin-modules": "^3.0.0",
		"esbuild": "^0.17.0",
		"obsidian": "^1.5.0",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	}
}